{
  "expo": {
    "name": "Memories",
    "slug": "memories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "memories",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fyi.ikigai.memories"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "widgets": [
        {
          "name": "Memories",
          "label": "Memories",
          "minWidth": "320dp",
          "minHeight": "120dp",
          "description": "Widgets for Strava",
          "previewImage": "./assets/widget-preview.png",
          "updatePeriodMillis": 1800000
        }
      ],
      "package": "fyi.ikigai.memories"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a59efe93-cc37-44be-bc55-24a6a773a143"
      }
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
